            "code": "Period"
          },
          {
            "extension": [
              {
                "url": "http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-keyelement",
                "valueBoolean": true
              }
            ],
            "code": "Timing"
          }
        ]
      },
      {
        "id": "ServiceRequest.authoredOn",
        "extension": [
          {
            "url": "http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-keyelement",
            "valueBoolean": true
          }
