        "min" : 0,
        "max" : "1",
        "type" : [{
          "code" : "Extension",
          "profile" : ["http://hl7.org/fhir/StructureDefinition/codeOptions"]
        }],
        "condition" : ["prc-1"]
      },
      {
        "id": "Procedure.subject",
        "extension": [
          {
            "url": "http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-keyelement",
