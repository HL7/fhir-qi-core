        "max" : "1",
        "type" : [{
          "code" : "Extension",
              "profile": [
                "http://hl7.org/fhir/5.0/StructureDefinition/extension-Procedure.recorded"
              ]
            }
          ]
      },
      {
        "id": "Procedure.basedOn",
